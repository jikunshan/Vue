{
	"name": "myApp",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build"
	},
	"dependencies": {
		"axios": "^0.19.2",
		"core-js": "^2.6.5",
		"element-ui": "^2.13.2",
		"vue": "^2.6.10",
		"vue-router": "^3.3.2"
	},
	"devDependencies": {
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@vue/cli-plugin-babel": "^3.8.0",
		"@vue/cli-service": "^5.0.8",
		"vue-template-compiler": "^2.6.10"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"@vue/standard"
		],
		"rules": {
			"indent": [
				"error",
				"tab"
			],
			"no-class-assign": 2,
			"semi": [
				2,
				"always"
			],
			"wrap-regex": 0
		},
		"parserOptions": {
			"parser": "babel-eslint"
		}
	}
}
